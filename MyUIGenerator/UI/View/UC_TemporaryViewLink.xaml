<UserControl x:Class="MyUIGenerator.View.UC_TemporaryViewLink"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
            >
    <Grid>
        <Button x:Name="btnLink" MinWidth="80"  Click="btnLink_Click" >
            <Button.Content>
                <StackPanel Orientation="Horizontal">
                    <Image Width="28" Height="28" Source="/MyUIGenerator;component/Images/EditForm.png"></Image>
                    <Label Content="Data Link"></Label>
                </StackPanel>
            </Button.Content>
        </Button>

    </Grid>
</UserControl>
